<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.potator"
		name="Watara - Supervision (Potator)"
		version="1.0.5.2"
		provider-name="Normmatt, Cal2, infval, Alekmaul">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms></platforms>
		<extensions>bin|sv</extensions>
		<supports_vfs>true</supports_vfs>
		<supports_standalone>false</supports_standalone>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en_GB">Watara - Supervision (Potator) 1.0.5 </summary>
		<description lang="en_GB">Potator is a Watara SuperVision emulator.

Supported files: .bin, .sv</description>
		<license>Public Domain</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.potator</source>
	</extension>
</addon>
